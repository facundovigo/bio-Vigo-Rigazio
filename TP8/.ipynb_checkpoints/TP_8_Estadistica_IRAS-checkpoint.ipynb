   "source": [
    "Q1 = df['Edad(meses)'].quantile(0.02)\n",
    "print(Q1)\n",
    "Q3 = df['Edad(meses)'].quantile(0.98)\n",
    "print(Q3)\n",
    "IQR = Q3 - Q1\n",
    "print(IQR)\n",
    "idx = ~((df['Edad(meses)'] < (Q1 - 1.5 * IQR)) | (df['Edad(meses)'] > (Q3 + 1.5 * IQR)))\n",
    "print(idx)"
   ]
